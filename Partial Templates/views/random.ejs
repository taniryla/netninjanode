<%- include('partials/head') %>

    <title>Random</title>
    </head>

    <body>
        <%- include('partials/nav') %>

            <h1>Your random number
                <%= num %>
            </h1>
            <% if(num % 2 === 0) { %>
                <h2>That is an even number!</h2>
                <% } else { %>
                    <h2>That is an odd number!</h2>
                    } %>
                    <% } %>

    </body>

    </html>